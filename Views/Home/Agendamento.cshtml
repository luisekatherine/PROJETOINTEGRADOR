<br>
<label asp-for="Telefone">▫️</label>
<input asp-for="Telefone" placeholder="Digite seu telefone:" required>
<br>
<label asp-for="Destino">▫️</label>
<input asp-for="Destino" placeholder="Digite o destino desejado:" required>
<br>
<label asp-for="DataDesejada">▫️</label>
<input asp-for="DataDesejada" placeholder="Escolha a data e hora" required>
<br>
<label asp-for="Passaporte">▫️</label>
<input asp-for="Passaporte" placeholder="Possui passaporte?" required>
<br>
<br>
<input type="submit" value="Agendar consultoria">
</form>
    </td>
    </tr>
    </div>
</table>
</body>
</html>